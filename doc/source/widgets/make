files = """CheckTable
ColorButton
DataTreeWidget
FileDialog
GradientWidget
GraphicsLayoutWidget
GraphicsView
HistogramLUTWidget
JoystickButton
MultiPlotWidget
PlotWidget
ProgressDialog
RawImageWidget
SpinBox
TableWidget
TreeWidget
