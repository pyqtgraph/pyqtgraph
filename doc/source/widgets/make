for f in files:
    print(f)
    fh = open(f.lower()+'.rst', 'w')
    fh.write(
"""%s
%s

.. autoclass:: pyqtgraph.%s
    :members:

    .. automethod:: pyqtgraph.%s.__init__

""" % (f, '='*len(f), f, f))
